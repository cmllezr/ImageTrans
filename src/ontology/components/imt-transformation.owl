                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/IAO_0000101"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/OBI_0000312"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/pmd/imt/IMT_0000003"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000101"/>
        <rdfs:label xml:lang="en">warped image</rdfs:label>
        <skos:definition xml:lang="en">Warped image is an image which is a specified output of an image transofrmation process. It is derived from an image, which is a source member of a matching pair, that is a specified output of a image transformation process.</skos:definition>
    </owl:Class>
