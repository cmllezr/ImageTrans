    <!-- https://w3id.org/pmd/imt/IMT_0000017 -->

    <owl:Class rdf:about="https://w3id.org/pmd/imt/IMT_0000017">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/co/PMD_0000583"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
