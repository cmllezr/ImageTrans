    <!-- https://w3id.org/pmd/imt/IMT_0000003 -->

    <owl:Class rdf:about="https://w3id.org/pmd/imt/IMT_0000003">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/co/PMD_0000583"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/OBI_0000293"/>
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/IAO_0000101"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/OBI_0000293"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/pmd/imt/IMT_0000019"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">image transformation</rdfs:label>
        <skos:altLabel xml:lang="en">image mapping</skos:altLabel>
        <skos:definition xml:lang="en">Image transformation process is a computing process which has some source  image and some parameterized transformation model, e.g., estimated homography, as specified inputs, and a warped image as a specified ouput. Here, the parameterized transformation model matches 2D coordinates of a scene of a source image to the coordinates of a scene of a target image.</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/imt/IMT_0000004 -->

    <owl:Class rdf:about="https://w3id.org/pmd/imt/IMT_0000004">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/imt/IMT_0000017"/>
        <rdfs:label xml:lang="en">manual point correspondences acquisition</rdfs:label>
        <skos:definition xml:lang="en">Manual point correspondences acquisition is point correspondences acquisition, where extraction and matching of key points in the images is performed by human efforts.</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/imt/IMT_0000005 -->

    <owl:Class rdf:about="https://w3id.org/pmd/imt/IMT_0000005">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000100"/>
        <obo:IAO_0000119>https://medium.com/all-things-about-robotics-and-computer-vision/homography-and-how-to-calculate-it-8abf3a13ddc5</obo:IAO_0000119>
        <rdfs:label xml:lang="en">point correspondences</rdfs:label>
        <skos:definition xml:lang="en">Point correspondences is data set which represent pairs of corresponding coordinates (x, y)  and (x´, y´) in two coordinate systems s and s´. The point correspondences are used to determine a mapping between s and s´ which can then be used to calculate a correspondences for all other coordinates from s to s´.</skos:definition>
    </owl:Class>
    


