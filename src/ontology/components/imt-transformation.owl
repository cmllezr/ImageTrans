    <!-- https://w3id.org/pmd/imt/IMT_0000021 -->

    <owl:Class rdf:about="https://w3id.org/pmd/imt/IMT_0000021">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/imt/IMT_0000005"/>
        <rdfs:label xml:lang="en">inlier point correspondence</rdfs:label>
        <skos:definition xml:lang="en">Inliers point correspondences are point correspondence, containing contains points which correctly identify the same physical location in both images. Such inliers fit the underlying transformation model within a predefined margin of error.</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/imt/IMT_0000022 -->

    <owl:Class rdf:about="https://w3id.org/pmd/imt/IMT_0000022">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/imt/IMT_0000005"/>
        <rdfs:label xml:lang="en">outlier point correspondence</rdfs:label>
        <skos:definition xml:lang="en">Outlier point correspondences are point correspondences, containing points which incorrectly identify the same physical location in both images, i.e., a point in a a source image is mistakenly matched to a different point in the target image.</skos:definition>
    </owl:Class>
    


