        <rdfs:label xml:lang="en">image transformation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/imt/IMT_0000004 -->

    <owl:Class rdf:about="https://w3id.org/pmd/imt/IMT_0000004">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/imt/IMT_0000003"/>
        <rdfs:label xml:lang="en">homography</rdfs:label>
        <rdfs:seeAlso>https://medium.com/wanabilini/homography-transformations-in-practice-974c9e2730b2</rdfs:seeAlso>
        <skos:definition xml:lang="en">A homography is a image transformation which has two images as specified inputs, where a coordinate system of one image has a target role, and another image has a source role, and the specified output of the transformation is another image, which maps the content of a sourse image to the 2D spatial region (coordinate system) of a target image.</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/imt/IMT_0000005 -->

    <owl:Class rdf:about="https://w3id.org/pmd/imt/IMT_0000005">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000027"/>
        <rdfs:label xml:lang="en">image transformation ground truth</rdfs:label>
        <skos:definition xml:lang="en">Image transformation ground truth is a data set which is a specified input to the homography process. It consists of pairs of 2D coordinates, where one element of the pair refers to a coordinate in a reference frame of a source a image, and the other element - to a coordinate in a reference frame of a target image.</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/imt/IMT_0000011 -->

    <owl:Class rdf:about="https://w3id.org/pmd/imt/IMT_0000011">
